@page
@model PrivacyModel
@{
  ViewData["Title"] = "Privacy Policy";
}
@section Breadcrumb {
  <div class="row heading-bg">
    <!-- Breadcrumb -->
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
      <ol class="breadcrumb">
        <li class="active"><span>@ViewData["Title"]</span></li>
